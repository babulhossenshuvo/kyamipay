{
  "name": "kpay/laravel-kpay-payment",
  "description": "Kyami Pay payment gateway integration for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kyami Pay",
      "email": "support@kyamipay.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "KPay\\LaravelKPayment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KPay\\LaravelKPayment\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KPay\\LaravelKPayment\\KPayServiceProvider"
      ],
      "aliases": {
        "KPay": "KPay\\LaravelKPayment\\Facades\\KPay"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
